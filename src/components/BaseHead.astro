---
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<head>
    <!--GLOBAL METADATA -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <!--FONT PRELOADS -->
    <link
        rel="preload"
        href="/fonts/GeistSans.woff2"
        as="font"
        type="font/woff2"
        crossorigin
    />
    <link
        rel="preload"
        href="/fonts/Segoe-UI.woff2"
        as="font"
        type="font/woff2"
        crossorigin
    />
    <title>{title}</title>
</head>
